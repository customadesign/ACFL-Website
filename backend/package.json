    "test:auth": "jest --testPathPattern='auth.test.ts'",
    "test:admin": "jest --testPathPattern='admin.test.ts'",
    "test:verbose": "jest --verbose",
    "setup-admin": "node scripts/createDefaultAdmin.js"
