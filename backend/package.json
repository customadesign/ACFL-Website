    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.1",
    "adminjs": "^7.6.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
